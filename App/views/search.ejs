    <!--content-->

    <section id="content">
      <div class="container">
        <h1><%= title%></h1>
        <div class="searchForm_box">


          <form id="search_form" action="search" role="form" method="post">

            <div class="form-group">
              <label for="username">Email</label>
              <input class="form-control" id="username" name="username" placeholder="Enter an Email">
            </div>
            <button type="submit" class="btn btn-primary">Search</button>
          </form>
        </div>

        <div class="form-group">
          <!-- <div class="forms" style="width:43%"> -->
          <h4>List of Users</h4>
          <table class="table" style="float: left">
            <thead>
              <tr>
                <th scope="col">User Id</th>
                <th scope="col">User Email</th>

              </tr>
            </thead>
            <tbody>
              <% for(var i=0; i<data.length; i++) { %>
                <tr>
                  <td>
                    <div id="thisUser" name = "userId">
                      <%= data[i].userid%>
                    </div>
                  </td>
                  <td><%= data[i].username%></td>
                  <td><button class="btn btn-info" onclick="showReviewForm()">Review</button></td>
                </tr>

                <% } %>
              </tbody>
            </table>
            <form id="review_form" action="search" role="form" method="post">
              <div class="form-group" >
                <div id="revieweeId" style="display: none; margin-bottom: 30px;">
                  <label for="revieweeId">UserId to be reviewed: </label>
                  <input class="form-control" id="revieweeId" name="revieweeId">
                </div>
                <div id="reviewContent" style="display: none; margin-bottom: 30px;">
                  <label for="reviewContent">Review User: </label>
                  <input class="form-control" id="reviewContent" name="reviewContent">
                </div>
                <div id="reviewRating" name="reviewRating" style="display: none; margin-bottom: 30px;">
                  <label for="reviewRating">Rate this User:</label>
                  <div class="form-check form-check-inline" style = "margin-bottom: 10px;">
                    <input class="form-check-input" id="reviewRating" type="radio" name="reviewRating" value="Negative">
                    <label class="form-check-label" for="inlineRadio1">Negative</label>
                  </div>
                  <div class="form-check form-check-inline" style = "margin-bottom: 10px;">
                    <input class="form-check-input" id="reviewRating" type="radio" name="reviewRating" value="Neutral">
                    <label class="form-check-label" for="inlineRadio2">Neutral</label>
                  </div>
                  <div class="form-check form-check-inline" style = "margin-bottom: 10px;">
                    <input class="form-check-input" id="reviewRating"  type="radio" name="reviewRating" value="Positive">
                    <label class="form-check-label" for="inlineRadio3">Positive</label>
                  </div>
                <button type="submit" class="btn btn-primary" onclick="createdReview()">Submit</button>
              </div>
            </div>

        </form>
      </div>
